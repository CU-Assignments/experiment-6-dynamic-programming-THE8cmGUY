class Solution:
    def maxProfit(self, prices: list[int]) -> int:
        min_price = float('inf')  # Set to a very high value
        max_profit = 0  # Start with 0 profit

        for price in prices:
            if price < min_price:
                min_price = price  # Update minimum price found so far
            else:
                max_profit = max(max_profit, price - min_price)  # Update max profit

        return max_profit
